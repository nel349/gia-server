{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node",
      "config": {
        "buildCommand": "npm run build",
        "outputDirectory": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/healthcheck",
      "dest": "/dist/app.js",
      "methods": ["GET"]
    },
    {
      "src": "/(.*)",
      "dest": "/dist/app.js"
    }
  ],
  "health": {
    "path": "/healthcheck"
  }
}